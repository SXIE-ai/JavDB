{
    "name": "汽油价格设置",
    "description": "设置查询地区和油号\n支持通知提醒功能",
    "icon": "oilcan.fill",
    "panels": [
        {
            "type": "select",
            "title": "地区",
            "key": "location",
            "options": [
                {"title": "湖南省", "value": "湖南"},
                {"title": "北京市", "value": "北京"},
                {"title": "上海市", "value": "上海"},
                {"title": "广东省", "value": "广东"},
                {"title": "浙江省", "value": "浙江"},
                {"title": "江苏省", "value": "江苏"},
                {"title": "四川省", "value": "四川"},
                {"title": "湖北省", "value": "湖北"},
                {"title": "山东省", "value": "山东"},
                {"title": "河南省", "value": "河南"}
            ],
            "default": "湖南"
        },
        {
            "type": "select",
            "title": "油号",
            "key": "type",
            "options": [
                {"title": "92号汽油", "value": "92"},
                {"title": "95号汽油", "value": "95"},
                {"title": "98号汽油", "value": "98"},
                {"title": "0号柴油", "value": "0"}
            ],
            "default": "92"
        },
        {
            "type": "switch",
            "title": "显示所有油号",
            "key": "isShowAll",
            "default": true
        },
        {
            "type": "switch",
            "title": "启用通知提醒",
            "key": "enableNotification",
            "default": true,
            "description": "定时任务执行时发送通知"
        }
    ]
}
