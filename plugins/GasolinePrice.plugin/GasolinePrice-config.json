{
    "name": "汽油价格设置",
    "description": "设置查询地区和油号",
    "icon": "oilcan.fill",
    "panels": [
        {
            "type": "input",
            "title": "地区",
            "key": "location",
            "placeholder": "如: 湖南, 北京, 上海, 广东",
            "default": "湖南"
        },
        {
            "type": "select",
            "title": "油号",
            "key": "type",
            "options": [
                {
                    "title": "92号汽油",
                    "value": "92"
                },
                {
                    "title": "95号汽油",
                    "value": "95"
                },
                {
                    "title": "98号汽油",
                    "value": "98"
                },
                {
                    "title": "0号柴油",
                    "value": "0"
                }
            ],
            "default": "92"
        },
        {
            "type": "switch",
            "title": "显示所有油号",
            "key": "isShowAll",
            "default": true
        }
    ]
}
